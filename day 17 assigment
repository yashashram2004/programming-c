#include <stdio.h>

int main() {
    int n,smallest;
    printf("Enter the size ");
    scanf("%d", &n);
    int arr[n];
    
    
    for(int i =0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    smallest = arr[0];
    
    for(int i = 0;i<n;i++){
        if(arr[i] < smallest){
            smallest = arr[i];
        }
    }
    printf("The smallest number is %d", smallest);

    return 0;
}
